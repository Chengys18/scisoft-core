<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="uk.ac.diamond.scisoft.applications.msmapper"
         point="org.eclipse.core.runtime.applications">
      <application
            cardinality="singleton-global"
            thread="main"
            visible="true">
         <run
               class="uk.ac.diamond.scisoft.applications.MillerSpaceMapperApplication">
         </run>
      </application>
   </extension>
   <extension
         id="MSMApp"
         point="org.eclipse.core.runtime.products">
      <product
            application="uk.ac.diamond.scisoft.applications.mapper"
            name="MSMApp">
         <property
               name="appName"
               value="MSMApp">
         </property>
      </product>
   </extension>
</plugin>
